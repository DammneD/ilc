{
  "name": "headline-news",
  "version": "1.0.0",
  "description": "Displays news articles",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel-node": "babel-node --presets=env",
    "dev": "concurrently --kill-others --kill-others-on-fail --names \"server,client\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch server.js --watch dist ./server.js",
    "dev:client": "nodemon -e js,mjs,json,vue --watch src --exec npm run build",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:client-spa && npm run build:server",
    "build:client": "webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:client-spa": "webpack --config config/webpack.client_spa.config.js --progress --hide-modules",
    "build:server": "webpack --config config/webpack.server.config.js --progress --hide-modules"
  },
  "author": "Okubanjo Oluwafunsho <me@devfunsho.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "cross-env": "^5.1.1",
    "express": "^4.16.2",
    "lru-cache": "^4.1.1",
    "single-spa-vue": "^1.5.2",
    "vue": "^2.5.3",
    "vue-meta": "^2.2.1",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.3",
    "vuex": "^3.0.1",
    "wrapper-webpack-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^4.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.4.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-merge": "latest",
    "webpack-node-externals": "^1.6.0"
  }
}
